   Private Sub grdMainLoad()
      Dim lngMonedaId As Long
      Dim lngPlanId As Long
      Dim oCompDetPlan As New clsCompDet(clsAppInfo.ConnectString)
      Dim oPlan As New clsPlan(clsAppInfo.ConnectString)

      Try
         Call moDataSetInit()

         For Each lngPlanId In moListaPlan
            With oPlan
               .PlanId = lngPlanId

               If .FindByPK Then
                  If ConMovimiento(lngPlanId) Then
                     mlngGrupo = PlanGrupoFind(lngPlanId)

                     If mlngMonedaId = 3 Then
                        lngMonedaId = PlanMonedaDefecto(lngPlanId)
                     Else
                        lngMonedaId = mlngMonedaId
                     End If

                     moDataTable.Rows.Add(GetRow(.DataSet.Tables(.TableName).Rows(0), lngMonedaId))
                  End If
               End If
            End With
         Next

         moDataTable.Rows.Add(GetFooter(lngMonedaId))

         grdMain.DataSource = moDataSet.Tables(moCompDet.TableName).DefaultView
         grdMain.RetrieveStructure()
         Call grdMainInit()

      Catch exp As Exception
         MessageBox.Show(exp.Message, Me.Text, MessageBoxButtons.OK, MessageBoxIcon.Exclamation)

      Finally
         oCompDetPlan.Dispose()
         oPlan.Dispose()
      End Try
   End Sub

   Private Sub moDataSetInit()
      moDataSet = New DataSet("Tablas")
      moDataTable = moDataSet.Tables.Add(moCompDet.TableName)
      Dim oCentroCosto As New clsCentroCosto(clsAppInfo.ConnectString)

      Try
         moDataTable.Columns.Add("PlanId", Type.GetType("System.Int32"))
         moDataTable.Columns.Add("PlanCta", Type.GetType("System.String"))
         moDataTable.Columns.Add("PlanDes", Type.GetType("System.String"))

         For Each lngCentroCostoId As Long In moListaCentroCosto
            With oCentroCosto
               .CentroCostoId = lngCentroCostoId

               If .FindByPK Then
                  moDataTable.Columns.Add(.CentroCostoId, Type.GetType("System.Decimal"))
               End If
            End With
         Next

         moDataTable.Columns.Add("Saldo", Type.GetType("System.Decimal"))

      Catch exp As Exception
         MessageBox.Show(exp.Message, Me.Text, MessageBoxButtons.OK, MessageBoxIcon.Exclamation)

      Finally
         oCentroCosto.Dispose()
      End Try
   End Sub

   Private Function GetRow(ByVal oDataRow As DataRow, ByVal lngMonedaId As Long) As DataRow
      Dim oRow As DataRow
      oRow = moDataTable.NewRow
      Dim oCentroCosto As New clsCentroCosto(clsAppInfo.ConnectString)

      Try
         oRow("PlanId") = oDataRow("PlanId")
         oRow("PlanCta") = oDataRow("PlanCta")
         oRow("PlanDes") = oDataRow("PlanDes")

         mdecSaldoPlan = 0

         For Each lngCentroCostoId As Long In moListaCentroCosto
            With oCentroCosto
               .CentroCostoId = lngCentroCostoId

               If .FindByPK Then
                  If mboolSaldoAnterior Then
                     mdecSaldoAnteriorCCO = SaldoAnteriorCentroCosto(oRow("PlanId"), lngCentroCostoId, lngMonedaId)
                  Else
                     mdecSaldoAnteriorCCO = 0
                  End If

                  mdecSaldo = SaldoCentroCosto(oRow("PlanId"), lngCentroCostoId, lngMonedaId)
                  mdecSaldo += mdecSaldoAnteriorCCO
                  mdecSaldoPlan += mdecSaldo
                  oRow("" & .CentroCostoId) = mdecSaldo
               End If
            End With
         Next

         oRow("Saldo") = mdecSaldoPlan

         Return oRow

      Catch exp As Exception
         MessageBox.Show(exp.Message, Me.Text, MessageBoxButtons.OK, MessageBoxIcon.Exclamation)

      Finally
         oCentroCosto.Dispose()
      End Try
   End Function

   Private Function GetFooter(ByVal lngMonedaId As Long) As DataRow
      Dim oDataRow As DataRow
      Dim oRow As DataRow
      oRow = moDataTable.NewRow
